<%= form_for :place, url: places_path do |f| %>

<p>
	<%= f.label :name %><br>
	<%= f.text_field :name %><br>
</p>

<p>
	<%= f.label :description %><br>
	<%= f.text_area :description %><br>
</p>

<p>
	<%= f.label :region %><br>
	<%= f.text_field :region %><br>
</p>

<p>
	<%= f.submit %>
</p>

<% end %>